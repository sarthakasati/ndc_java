package com.ndc.request;

import com.fasterxml.jackson.annotation.JsonProperty;

public class Relationship {
    @JsonProperty("type")
    private String type; // Type of relationship, e.g., "object" or "array"

    @JsonProperty("source_collection")
    private String sourceCollection; // The parent collection

    @JsonProperty("relationship_def")
    private RelationshipDef relationshipDef; // Definition of the relationship

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getSourceCollection() {
        return sourceCollection;
    }

    public void setSourceCollection(String sourceCollection) {
        this.sourceCollection = sourceCollection;
    }

    public RelationshipDef getRelationshipDef() {
        return relationshipDef;
    }

    public void setRelationshipDef(RelationshipDef relationshipDef) {
        this.relationshipDef = relationshipDef;
    }
}

